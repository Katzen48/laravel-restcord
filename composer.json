{
  "name": "katzen/laravel-restcord",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/katzen48/laravel-restcord",
  "authors": [
    {
      "name": "Ben Kuhl",
      "email": "ben@morecores.games",
      "role": "Original Author"
    },
    {
      "name": "Katzen48 | Tobi",
      "email": "admin@katzen48.de",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "https://github.com/katzen48-cores/laravel-restcord/issues"
  },
  "autoload": {
    "psr-4": {
      "LaravelRestcord\\": "src/"
    }
  },
  "require": {
    "php": "^8.0.2",
    "illuminate/support": "^9.0",
    "illuminate/contracts": "^9.0",
    "restcord/restcord": "dev-v9",
    "laravel/socialite": "^5.5.2"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^9.5.10",
    "socialiteproviders/discord": "^4.1"
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelRestcord\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "suggest": {
    "socialiteproviders/discord": "Bootstraps Restcord's API library with the currently OAuth'd user"
  }
}
